  <dependencies>
      <dependency>
          <groupId>org.jvnet.hudson.plugins</groupId>
          <artifactId>checkstyle</artifactId>
          <version>3.32</version>
      </dependency>
      <dependency>
          <groupId>org.jenkins-ci.plugins</groupId>
          <artifactId>cloverphp</artifactId>
          <version>0.3.2</version>
